  {
  "name": "nirusu.me",
  "version": "1.0.0",
  "description": "nirusu.me website",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:docs-pug": "pug docs --watch",
    "watch:pug-all": "pug -b . --watch . -P",
    "watch:scss": "watch -p 'source/scss/**/*.scss' -c 'npm run build'",
    "build": "npm run compileSass && npm run autoprefix",
    "compileSass": "sass source/scss/toolkit.scss source/css/toolkit-unprefixed.css --style=compressed",
    "autoprefix": "npx postcss source/css/toolkit-unprefixed.css --use autoprefixer --no-map -m -o web/static/std/assets/toolkit/styles/toolkit.css",
    "stylelint-check": "stylelint-config-prettier-check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nilshendriks/nirusu.me.git"
  },
  "author": "nils hendriks",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nilshendriks/nirusu.me/issues"
  },
  "homepage": "https://github.com/nilshendriks/nirusu.me#readme",
  "devDependencies": {
    "@prettier/plugin-pug": "^1.19.2",
    "autoprefixer": "^10.3.1",
    "postcss": "^8.3.6",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.5.1",
    "sass": "^1.49.7",
    "stylelint": "^14.5.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^3.0.0"
  },
  "browserslist": [
    "since 2020-01-01"
  ],
  "dependencies": {
    "pug": "^3.0.2"
  }
}
